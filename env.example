# Database Configuration
# Для локального запуску використовуйте:
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/kiosk_db

# Якщо у вас інший пароль для PostgreSQL, змініть:
# DATABASE_URL=postgresql://postgres:ВАШ_ПАРОЛЬ@localhost:5432/kiosk_db

# JWT Secret
# Генеруйте випадковий рядок мінімум 32 символи
# Можна використати: openssl rand -base64 32
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-min-32-chars

# Server Port (опціонально, за замовчуванням 3001)
PORT=3001

# Environment
NODE_ENV=development

# Telegram Bot Token (опціонально)
# Створіть бота через @BotFather в Telegram та додайте токен тут
# TELEGRAM_BOT_TOKEN=your_bot_token_here
